{
	"extends": "./tsconfig.json",
	"include": [
		"vite.config.ts",
		"react-router.config.ts",
		"playwright.config.ts",
		"e2e/**/*",
		"plugins/**/*.ts"
	],
	"compilerOptions": {
		"composite": true,
		"strict": true,
		"types": ["node"],
		"lib": ["ES2022"],
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"rootDirs": [".", "./.react-router/types"]
	}
}
